journal = {Genome Research},
month = {mar},
number = {3},
pages = {568--576},
pmid = {22300766},
title = {{VarScan 2: Somatic mutation and copy number alteration discovery in cancer by exome sequencing}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22300766 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3290792 http://genome.cshlp.org/cgi/doi/10.1101/gr.129684.111},
