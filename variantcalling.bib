doi = {10.1016/j.ccr.2013.11.002},
file = {:Users/brianmannakee/Library/Application Support/Mendeley Desktop/Downloaded/Chen et al. - 2013 - Targeting Oxidative Stress in Embryonal Rhabdomyosarcoma.pdf:pdf},
