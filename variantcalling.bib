number = {283},
pages = {283ra54},
pmid = {25877892},
publisher = {American Association for the Advancement of Science},
title = {{Clonal status of actionable driver events and the timing of mutational processes in cancer evolution.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25877892 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4636056},
